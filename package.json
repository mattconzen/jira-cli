{
  "name": "jira",
  "description": "jira-cli",
  "version": "0.1.0",
  "author": "Matt Conzen @mattconzen",
  "bin": {
    "jira": "./bin/run"
  },
  "bugs": "https://github.com/mattconzen/jira-cli/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^1",
    "@oclif/plugin-update": "^1.2.6",
    "@types/fs-extra": "^5.0.2",
    "@types/request": "^2.47.0",
    "aws-sdk": "^2.247.1",
    "colors": "^1.2.5",
    "fs-extra": "^6.0.1",
    "global": "^4.3.2",
    "request": "^2.86.0",
    "tslib": "^1",
    "web-request": "^1.0.7"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.13.23",
    "@oclif/test": "^1",
    "@oclif/tslint": "^1",
    "@types/chai": "^4",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "chai": "^4",
    "globby": "^8",
    "husky": "^0.14.3",
    "lint-staged": "^7.1.1",
    "mocha": "^5",
    "nyc": "^11.8.0",
    "prettier": "^1.12.1",
    "ts-node": "^6",
    "tslint": "^5",
    "tslint-config-prettier": "^1.13.0",
    "typescript": "^2.8"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/mattconzen/jira-cli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "jira",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-update"
    ],
    "macos": {
      "identifier": "com.mattconzen.jiracli"
    },
    "update": {
      "s3": {
        "bucket": "jira-cli-releases",
        "host": "http://jira-cli-releases.s3-website-us-west-2.amazonaws.com"
      },
      "node": {
        "version": "8.9.1",
        "targets": [
          "darwin-x64",
          "linux-x64",
          "win32-x64"
        ]
      }
    }
  },
  "repository": "mattconzen/jira-cli",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "tsc -p test --noEmit && tslint -p test -t stylish",
    "prepack": "rm -rf lib && tsc && oclif-dev manifest && oclif-dev readme",
    "test": "nyc mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "types": "lib/index.d.ts"
}
